{
  "include": [".astro/types.d.ts", "**/*"],
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@c/*": ["src/components/*"],
      "@js/*": ["src/js/*"],
      "@svg/*": ["src/components/svg/*"],
      "@package/*": ["package/*"],
      "@slider/*": ["src/wip/slider/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "react",
    "outDir": "./dist"
  }
}
